<script>
    var showCollection = [].slice.call(document.querySelectorAll('body'));

    var isTrig1 = false, isTrig2 = false;
    var performShow = function () {
        showCollection.forEach(function (el) {
            el.style.opacity = 1;
        });
    };
    document.querySelector('body').style.opacity = 0;
    //    document.addEventListener('DOMContentLoaded', function () {
    //        showCollection.forEach(function (el) {
    //            el.style.opacity = 0;
    //        });
    //    });
    //
    setTimeout(function () {
        isTrig1 = true;
        if (isTrig2) {
            performShow();
        }
    }, 600);
    window.addEventListener('load', function () {
        isTrig2 = true;
        if (isTrig1) {
            performShow();
        }
    });
</script>

